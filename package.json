{
    "name": "@cleverjs/cli",
    "version": "2.1.2",
    "module": "./dist/esm/index.js",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "scripts": {
        "typecheck": "tsc -p ./tsconfig/tsconfig.typecheck.json && echo 'Typecheck passed!'",
        "test": "bun test",
        "build": "bun run build:esm && bun run build:cjs && bun run build:types",
        "build:esm": "rm -rf dist/esm && tsc -p ./tsconfig/tsconfig.esm.json",
        "build:cjs": "rm -rf dist/cjs && tsc -p ./tsconfig/tsconfig.cjs.json",
        "build:types": "rm -rf dist/types && tsc -p ./tsconfig/tsconfig.types.json",
        "release": "bun run test && bun run build && bun publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeiCraftMC/CleverJS-CLI"
    },
    "author": "LeiCraft_",
    "publisher": "LeiCraft_MC",
    "license": "AGPL-3.0-only",
    "bugs": {
        "url": "https://github.com/LeiCraftMC/CleverJS-CLI/issues"
    },
    "keywords": [
        "JavaScript",
        "TypeScript",
        "CLI",
        "CleverJS",
        "Terminal",
        "Command"
    ],
    "homepage": "https://github.com/LeiCraftMC/CleverJS-CLI#readme",
    "devDependencies": {
        "@types/bun": "^1.2.4",
        "@types/node": "^22.13.5",
        "typescript": "^5.7.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@cleverjs/utils": "^1.1.9"
    }
}
